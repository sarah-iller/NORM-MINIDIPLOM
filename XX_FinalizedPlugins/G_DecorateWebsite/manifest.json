{
  "manifest_version": 3,
  "name": "Web Decoration Extension",
  "version": "1.0",
  "description": "Adds a decorative div with an image to the page.",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "style.css"
      ],
      "html": [
        "snippet.html"
      ]
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "snippet.html",
        "images/knot_01.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}