{
  "manifest_version": 3,
  "name": "Current Experiment_I",
  "version": "1.0",
  "description": "",
  "host_permissions": [
    "<all_urls>"
  ],
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "content.js"
      ],
      "css": [
        "content.css"
      ]
    }
  ],
  "permissions": [
    "scripting"
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "svgs/*.svg"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}